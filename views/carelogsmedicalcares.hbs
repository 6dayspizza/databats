<main>
    <div id="browse">
    <table id="carelogsmedicalcares_table">
        <caption></caption>
        {{!-- Header section --}}
        <thead>
            {{!-- For just the first row, we print each key of the row object as a header cell so we
            know what each column means when the page renders --}}
            <tr>
                <th width=4%></th>
                <th width=4%>id</th>
                <th>idCareLog</th>
                <th>idMedicalCare</th>
            </tr>
        </thead>

            {{!-- Body section --}}
        <tbody>
            {{#each carelogsmedicalcares}}
            <tr>
                <td></td>
                <td>{{this.idCareLogMedicalCare}}</td>
                <td>{{this.idCareLog}}</td>
                <td>{{this.idMedicalCare}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    </main>
    <hr>
</div>
